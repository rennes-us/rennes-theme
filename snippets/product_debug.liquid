<div class="debug">title: {{product.title}}
handle: {{product.handle}}
id: {{product.id}}
template_suffix: {{product.template_suffix}}
available: {{product.available}}
url: {{product.url}}
type: {{product.type}}
vendor: {{product.vendor}}
has_only_default_variant: {{product.has_only_default_variant}}
first_available_variant title: {{product.first_available_variant.title}}
selected_variant title: {{product.selected_variant.title}}
selected_or_first_available_variant title: {{product.selected_or_first_available_variant.title}}
price: {{product.price}}
price_min: {{product.price_min}}
price_max: {{product.price_max}}
price_varies: {{product.price_varies}}
compare_at_price: {{product.compare_at_price}}
compare_at_price_min: {{product.compare_at_price_min}}
compare_at_price_max: {{product.compare_at_price_max}}
compare_at_price_varies: {{product.compare_at_price_varies}}
created_at: {{product.created_at}}
published_at: {{product.published_at}}

collections ({{product.collections.size}}):
{%- for collection in product.collections %}
  {{collection.title}}{% endfor %}

tags ({{product.tags.size}}):
{%- for tag in product.tags %}
  {{tag}}{% endfor %}
options ({{ product.options.size }}):
{%- for option in product.options %}
  {{ option }}{% endfor %}

variants ({{ product.variants.size}}):
{%- for variant in product.variants %}
  "{{variant.title}}"
    title: {{variant.title}}
    id: {{variant.id}}
    url {{variant.url}}
    weight: {{variant.weight}}
    weight_unit: {{variant.weight_unit}}
    weight_in_unit: {{variant.weight_in_unit}}
    sku: {{variant.sku}}
    taxable: {{variant.taxable}}
    selected: {{variant.selected}}
    image: {{variant.image}}
    incoming: {{variant.incoming}}
    available: {{variant.available}}
    barcode: {{variant.barcode}}
    price: {{variant.price}}
    compare_at_price: {{variant.compare_at_price}}
    inventory_quantity: {{variant.inventory_quantity}}
    inventory_policy: {{variant.inventory_policy}}
    inventory_management: {{variant.inventory_management}}
    next_incoming_date: {{variant.next_incoming_date}}
    option1: {{variant.option1}}
    option2: {{variant.option2}}
    option3: {{variant.option3}}
    requires_shipping: {{variant.requires_shipping}}{% endfor %}

images ({{ product.images.size}}):
{%- for img in product.images %}
  {{img}}{% endfor %}

description:
  {{product.description}}
</div>
